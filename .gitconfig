[user]
	email = saoirse-zee@users.noreply.github.com
	name = Zachary Smith

[color]
    ui = true

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = red

[alias]
    co = checkout
    cob = checkout -b
    c = commit
    cm = commit -m
    cam = commit -am
    st = status
    br = branch
    s = status -s
	lg = "log --oneline --decorate --all --"
[push]
	default = simple
[core]
	excludesfile = /Users/zacharysmith/.gitignore_global
        editor = vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
